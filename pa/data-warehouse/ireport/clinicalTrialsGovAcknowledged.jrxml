	<queryString>
		<![CDATA[SELECT soi.extension, si.open_date, si.admin, si.scientific,
       si.close_date, si.admin_close_date, si.scientific_close_date,
       ru.first_name AS ru_first_name, ru.last_name AS ru_last_name,
       cu.first_name AS cu_first_name, cu.last_name AS cu_last_name
FROM study_inbox si
JOIN study_otheridentifiers soi ON (si.study_protocol_identifier = soi.study_protocol_id)
LEFT JOIN registry_user ru ON (ru.csm_user_id = si.user_last_updated_id)
LEFT JOIN csm_user cu ON (cu.user_id = si.user_last_updated_id)
WHERE si.comments LIKE 'Trial has been updated from ClinicalTrials.gov%'
  AND (si.close_date IS NOT NULL OR si.admin_close_date IS NOT NULL OR si.scientific_close_date IS NOT NULL)
  AND soi.root = '2.16.840.1.113883.3.26.4.3'
  AND ($P{Start_Date_o}::DATE IS NULL
       OR $P{Start_Date_o}::DATE <= date_trunc('day', si.close_date)
       OR $P{Start_Date_o}::DATE <= date_trunc('day', si.admin_close_date)
       OR $P{Start_Date_o}::DATE <= date_trunc('day', si.scientific_close_date)
      )
  AND ($P{End_Date_o}::DATE IS NULL
       OR $P{End_Date_o}::DATE >= date_trunc('day', si.close_date)
       OR $P{End_Date_o}::DATE >= date_trunc('day', si.admin_close_date)
       OR $P{End_Date_o}::DATE >= date_trunc('day', si.scientific_close_date)
      )
  AND ($P{NCI_Trial_ID_po} IS NULL OR soi.extension LIKE ( '%' || $P{NCI_Trial_ID_po} || '%'))
ORDER BY soi.extension, si.open_date, si.close_date]]>
	</queryString>
