	<field name="cu_first_name" class="java.lang.String"/>
	<field name="cu_last_name" class="java.lang.String"/>
	<variable name="name" class="java.lang.String">
		<variableExpression><![CDATA[(org.apache.commons.lang.StringUtils.isBlank($F{cu_last_name}))
 ? (org.apache.commons.lang.StringUtils.isBlank($F{ru_first_name})
    ? $F{ru_last_name} : ($F{ru_first_name} + " " + $F{ru_last_name}))
 : (org.apache.commons.lang.StringUtils.isBlank($F{cu_first_name})
    ? $F{cu_last_name} : ($F{cu_first_name} + " " + $F{cu_last_name}))]]></variableExpression>
	</variable>
