<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd"
             version="6">
  <display-name>pa-ear</display-name>
  <module>
    <web>
      <web-uri>reg-web.war</web-uri>
      <context-root>/registry</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>pa-web.war</web-uri>
      <context-root>/pa</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>pa-webservices.war</web-uri>
      <context-root>/services</context-root>
    </web>
  </module>  
  <module>
    <ejb>pa-ejb.jar</ejb>
  </module>
  <library-directory>lib</library-directory>
  <security-role><role-name>Abstractor</role-name></security-role>
  <security-role><role-name>AdminAbstractor</role-name></security-role>
  <security-role><role-name>RegAdmin</role-name></security-role>
  <security-role><role-name>ReportViewer</role-name></security-role>
  <security-role><role-name>ScientificAbstractor</role-name></security-role>
  <security-role><role-name>SiteAdmin</role-name></security-role>
  <security-role><role-name>SuAbstractor</role-name></security-role>
  <security-role><role-name>Submitter</role-name></security-role>
  <security-role><role-name>ResultsAbstractor</role-name></security-role>
  <security-role><role-name>SecurityAdmin</role-name></security-role>
  <security-role><role-name>ProgramCodeAdministrator</role-name></security-role>
</application>